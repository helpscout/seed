{
  "name": "@seedcss/seed-navbar",
  "version": "0.0.6",
  "description": "Navbar component pack for Seed",
  "main": "index.js",
  "watch": {
    "test": {
      "patterns": [
        "scss",
        "test"
      ],
      "extensions": "scss, js",
      "quiet": true
    }
  },
  "scripts": {
    "banner": "node ./scripts/banner.js",
    "build:main": "node ./scripts/build.js",
    "build": "npm run build:main && npm run banner",
    "test:mocha": "mocha",
    "test": "npm run test:mocha",
    "watch": "npm-watch"
  },
  "homepage": "https://github.com/helpscout/seed/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/seed.git"
  },
  "authors": [
    "ItsJonQ <hello@jonquach.com>"
  ],
  "license": "MIT",
  "keywords": [
    "bem",
    "css",
    "design",
    "functional",
    "itcss",
    "navbar",
    "nav",
    "modular",
    "oocss",
    "performance",
    "seed",
    "seed-pack",
    "scss"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "@seedcss/seed-button": "^0.0.6",
    "@seedcss/seed-dropdown": "^0.0.6",
    "@seedcss/seed-nav": "^0.0.6",
    "@seedcss/seed-publish": "^0.0.6",
    "sass-pathfinder": "0.0.5"
  },
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.3",
    "seed-barista": "^1.1.0",
    "seed-bistro": "^0.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "prepublish": "npm run build",
  "bugs": {
    "url": "https://github.com/helpscout/seed/issues"
  },
  "postversion": "npm run build",
  "prepack": "npm run build"
}
