{
  "name": "@seedcss/seed-navbar",
  "version": "0.0.9",
  "description": "Navbar component pack for Seed",
  "main": "index.js",
  "watch": {
    "test": {
      "patterns": [
        "scss",
        "test"
      ],
      "extensions": "scss, js",
      "quiet": true
    }
  },
  "scripts": {
    "banner": "node ./scripts/banner.js",
    "build:main": "node ./scripts/build.js",
    "build": "npm run build:main && npm run banner",
    "test:mocha": "mocha",
    "test": "npm run build",
    "watch": "npm-watch"
  },
  "homepage": "https://github.com/helpscout/seed/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/seed.git"
  },
  "license": "MIT",
  "keywords": [
    "bem",
    "css",
    "design",
    "functional",
    "itcss",
    "navbar",
    "nav",
    "modular",
    "oocss",
    "performance",
    "seed",
    "seed-pack",
    "scss"
  ],
  "dependencies": {
    "@seedcss/seed-button": "^0.0.9",
    "@seedcss/seed-dropdown": "^0.0.9",
    "@seedcss/seed-nav": "^0.0.9",
    "@seedcss/seed-publish": "^0.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "prepublish": "npm run build",
  "bugs": {
    "url": "https://github.com/helpscout/seed/issues"
  }
}
