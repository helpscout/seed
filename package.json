{
  "name": "seed-typography",
  "version": "0.0.1",
  "description": "Main typography pack for Seed",
  "main": "index.js",
  "homepage": "https://github.com/helpscout/seed-typography",
  "scripts": {
    "banner": "node ./scripts/banner.js",
    "build": "npm run build:main && npm run build:min && npm run banner",
    "build:main": "node-sass --include-path node_modules/seed-breakpoints/scss scss/pack/_seed-typography.scss --include-path node_modules/seed-props/scss dist/seed-typography.css",
    "build:min": "node-sass --include-path node_modules/seed-breakpoints/scss --include-path node_modules/seed-props/scss scss/pack/_seed-typography.scss dist/seed-typography.min.css --output-style compressed",
    "test": "node ./scripts/test.js && npm run build",
    "prepublish": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/seed-typography.git"
  },
  "authors": [
    "ItsJonQ <itsjonq@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helpscout/seed-typography/issues"
  },
  "keywords": [
    "bem",
    "css",
    "design",
    "functional",
    "itcss",
    "modular",
    "oocss",
    "performance",
    "seed",
    "seed-pack",
    "scss",
    "type"
  ],
  "engines": {
    "node": ">=0.10.1"
  },
  "devDependencies": {
    "node-sass": "^3.8.0"
  },
  "dependencies": {
    "seed-breakpoints": "0.1.0",
    "seed-props": "0.0.2"
  }
}
