{
  "name": "seed-props",
  "version": "0.0.2",
  "description": "Props mixin pack for Seed",
  "main": "index.js",
  "homepage": "https://github.com/helpscout/seed-props",
  "scripts": {
    "banner": "node ./scripts/banner.js",
    "build": "npm run build:main && npm run build:min && npm run banner",
    "build:main": "node-sass scss/pack/_seed-props.scss dist/seed-props.css",
    "build:min": "node-sass scss/pack/_seed-props.scss dist/seed-props.min.css --output-style compressed",
    "test": "node ./scripts/test.js && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/seed-props.git"
  },
  "authors": [
    "ItsJonQ <itsjonq@gmail.com>"
  ],
  "keywords": [
    "bem",
    "callback",
    "content",
    "css",
    "design",
    "functional",
    "itcss",
    "modular",
    "oocss",
    "performance",
    "props",
    "seed",
    "seed-pack",
    "scss"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helpscout/seed-props/issues"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "node-sass": "^3.8.0"
  }
}
