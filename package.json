{
  "name": "@seedcss/seed",
  "version": "0.0.7",
  "description": "A component-first CSS design system.",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist --no-ci",
    "bootstrap:ci": "lerna bootstrap --no-ci",
    "build:all": "lerna run build",
    "build": "node ./scripts/build.js --write",
    "build:release": "npm run update && npm run setup && npm run build",
    "build:test": "node ./scripts/build.js",
    "clean": "lerna clean",
    "clean:lock": "find ./packages -type f -name \"package-lock.json\" -delete",
    "diff": "lerna diff",
    "postinstall": "npm run update",
    "release:before": "npm run setup && npm run update && npm run build",
    "release": "npm run release:before && lerna publish",
    "start": "npm run bootstrap && npm run storybook",
    "setup": "node ./scripts/setup.js",
    "test": "npm run build:test",
    "update": "node ./scripts/update.js",
    "storybook": "start-storybook -p 7200",
    "build-storybook": "build-storybook"
  },
  "homepage": "https://github.com/helpscout/seed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/seed.git"
  },
  "author": "ItsJonQ",
  "contributors": [
    "jaredmcdaniel",
    "thefoxis"
  ],
  "license": "MIT",
  "keywords": [
    "bem",
    "component",
    "components",
    "css",
    "design",
    "functional",
    "helpscout",
    "itcss",
    "modular",
    "oocss",
    "performance",
    "seed",
    "scss",
    "system"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "hoek": "^5.0.3",
    "lerna": "^3.4.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sass-loader": "^7.1.0",
    "sass-pathfinder": "^0.0.5",
    "seed-barista": "^1.1.1",
    "seed-bistro": "^0.2.1"
  },
  "dependencies": {}
}
