{
  "name": "seed-typography",
  "version": "0.0.1",
  "description": "Typography utility pack for Seed",
  "main": "index.js",
  "homepage": "",
  "scripts": {
    "banner": "node ./scripts/banner.js",
    "build": "npm run build:main && npm run build:min && npm run banner",
    "build:main": "node-sass scss/pack/_seed-typography.scss dist/seed-typography.css",
    "build:min": "node-sass scss/pack/_seed-typography.scss dist/seed-typography.min.css --output-style compressed",
    "test": "node ./scripts/test.js && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "authors": [
  ],
  "license": "MIT",
  "engines": {
    "node": ">=0.10.1"
  },
  "devDependencies": {
    "node-sass": "^3.8.0"
  }
}
